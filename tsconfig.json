{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./compilation",
    "strict": true,
    "declaration": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "sourceRoot": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "types": [ "node" ],
  "exclude": [
    "redux.ts",
  ]
}
